::
  @x0&\n\$
  @y0&\n\$
  @x1&\n\$
  @y1&\n\$

:clone+\!%.Rect\
  => &%.Rect(@x0, @y0, @x1, @y1)

:w+\n\
  => @.x1 - @.x0

:h+\n\
  => @.y1 - @.y0

:p00+\!%.Point\
  => &%.Point(@.x0, @.y0)
:p01+\!%.Point\
  => &%.Point(@.x0, @.y1)
:p10+\!%.Point\
  => &%.Point(@.x1, @.y0)
:p11+\!%.Point\
  => &%.Point(@.x1, @.y1)
