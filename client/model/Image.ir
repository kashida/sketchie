::<%.Item
  el\!Element\?
  @img+\!Image\ (el && \!Image\(el.firstChild)) || &Image()
  ^(el)
  @.element.style['-webkit-user-select'] = 'none'
  @.element.appendChild(@img)

:serializable+\O\
  obj := @.baseSerializable
  obj.src = @.src
  => obj

:createFromObj = ##
  \%.Image\
  obj\!O\$
  if !obj.src
    => null
  img := &%.Image()
  img.src = obj.src
  => img

:src+\s\
  => @img.src
:src*
  val\s\$
  @img.src = val

:width+\n\
  => @img.width

:height+\n\
  => @img.height
