::<%.Set
  actions := [#]
    &%.Group('add', [#])
      &%.NewDraw()
      &%.NewText()
      &%.Paste()
    &%.Save()
    &%.BackToList()
    &%.Group('zoom', [#])
      &%.ZoomIn()
      &%.ZoomOut()
      &%.ZoomDefault()
      &%.ZoomShowAll()
    &%.Group('select', [#])
      &%.ExpandSelectionDown()
      &%.ExpandSelectionUp()
      &%.MoveSelectionDown()
      &%.MoveSelectionUp()
  ^(actions, &ui.GlobalBox())
