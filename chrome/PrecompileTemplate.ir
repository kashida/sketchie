::
  @template_file\s\ 'static/page.html'

:@precompile\s\
  // Load the template file.
  if !_fs.existsSync(@template_file)
    throw @template_file + ' not found.'
  tmpl := _fs.readFileSync(@template_file, 'utf8')

  // Render.
  => _.template(tmpl).source

:code
  => [#].join('\n')
    'var Templates = {'
    'page: '
    @precompile()
    '};'
